{
    "name": "context-clips",
    "version": "1.0.0",
    "description": "Context-aware clipboard manager for Chrome that intelligently organizes your copied content",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "type-check": "tsc --noEmit",

    },
    "devDependencies": {
        "@crxjs/vite-plugin": "^2.0.0-beta.21",
        "@types/chrome": "^0.0.251",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@vitejs/plugin-react": "^4.3.0",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2",
        "vite": "^4.4.9"
    }
